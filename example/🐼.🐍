import pandas as 🐼

# Define a dataframe and print it to the console
🐍 = 🐼.DataFrame(
    {
        "animal": ["Panda", "Python", "Lion"],
        "😀": ["🐼", "🐍", "🦁"],
        "number": [1, 2, 3],
    },
).set_index("😀")
print(🐍)

# Do some arithmetic with the dataframe
🔤 = f" Pythonji {' '.join(🐍.index)}"
🔢 = 🐍.loc["🐍"].number + 🐍.loc["🐼"].number
print(🔤 * 🔢)
